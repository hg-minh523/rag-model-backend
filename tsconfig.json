{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./src", // <--- Set baseUrl to src
    "paths": {
      "src/*": ["./*"],
      "tiktok_nodejs_sdk": ["./node_modules/tiktok_nodejs_sdk"]
    },
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,"esModuleInterop": true,
    "noFallthroughCasesInSwitch": false
  }
}
